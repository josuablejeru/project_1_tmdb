{
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "/opt/conda/bin/black",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "isort.args": [
    "--profile",
    "black"
  ],
  "cSpell.words": [
    "astype",
    "fillna",
    "iloc"
  ],
}